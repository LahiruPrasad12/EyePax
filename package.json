    "@capacitor/ios": "3.5.1",
    "@capacitor/keyboard": "1.2.0",
    "@capacitor/status-bar": "1.0.6",
    "@ionic/core": "^6.0.3",
    "@ionic/vue": "^6.0.1",
    "@ionic/vue-router": "^6.0.1",
    "@ionited/tooltip-vue": "^0.1.2",
    "@types/vue-router": "^2.0.0",
    "axios": "^0.25.0",
